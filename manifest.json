{
  "manifest_version": 2,
  "name": "Tmall-tickets",
  "version": "1.2",
  "description": "天猫超市抢购茅台",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://cart.tmall.com/*","https://buy.tmall.com/*","https://cart.taobao.com/*","https://buy.taobao.com/*"],
      "js": ["maotai.js"],
      "run_at": "document_idle"
    }
  ]
}